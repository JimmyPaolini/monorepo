{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "cacheDirectory": ".nx/cache",
  "defaultBase": "main",
  "generators": {
    "@nx/react": {
      "application": {
        "babel": true,
        "bundler": "vite",
        "linter": "eslint",
        "style": "tailwind"
      },
      "component": {
        "style": "tailwind"
      },
      "library": {
        "linter": "eslint",
        "style": "tailwind"
      }
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/vitest.config.base.ts",
      "!{projectRoot}/src/test-setup.[jt]s"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/nx.json",
      "{workspaceRoot}/tsconfig.base.json",
      "{workspaceRoot}/pnpm-lock.yaml",
      "{workspaceRoot}/.github/workflows/*.yml"
    ]
  },
  "parallel": 3,
  "plugins": [
    {
      "options": {
        "buildDepsTargetName": false,
        "buildTargetName": "build",
        "devTargetName": "develop",
        "e2eTestTargetName": false,
        "previewTargetName": false,
        "serveStaticTargetName": false,
        "serveTargetName": false,
        "typecheckTargetName": "typecheck",
        "watchDepsTargetName": false
      },
      "plugin": "@nx/vite/plugin"
    },
    {
      "options": {
        "targetName": "lint"
      },
      "plugin": "@nx/eslint/plugin"
    }
  ],
  "projects": {
    "caelundas": {
      "tags": []
    },
    "monorepo": {
      "tags": ["scripts"]
    }
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"]
    },
    "code-analysis": {
      "cache": true,
      "configurations": {
        "check": {
          "parallel": true
        },
        "write": {
          "parallel": false
        }
      },
      "defaultConfiguration": "check",
      "executor": "nx:run-commands",
      "options": {
        "outputStyle": "stream"
      }
    },
    "dependency-analysis": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": [
        "default",
        "^default",
        "{projectRoot}/**/*.ts",
        "{projectRoot}/**/*.tsx",
        "{workspaceRoot}/.dependency-cruiser.cjs"
      ],
      "options": {
        "command": "depcruise {projectRoot}/src --config .dependency-cruiser.cjs",
        "cwd": "{workspaceRoot}"
      }
    },
    "format": {
      "cache": true,
      "configurations": {
        "check": {
          "args": "--check"
        },
        "write": {
          "args": "--write"
        }
      },
      "defaultConfiguration": "write",
      "executor": "nx:run-commands",
      "inputs": [
        "default",
        "{workspaceRoot}/prettier.config.ts",
        "{workspaceRoot}/.prettierignore"
      ],
      "options": {
        "command": "prettier {args} {projectRoot}"
      }
    },
    "knip": {
      "cache": true,
      "configurations": {
        "check": {
          "command": "NODE_OPTIONS='--import=tsx/esm' knip --workspace {projectRoot}"
        },
        "write": {
          "command": "NODE_OPTIONS='--import=tsx/esm' knip --fix --workspace {projectRoot}"
        }
      },
      "defaultConfiguration": "check",
      "executor": "nx:run-commands",
      "inputs": ["default", "{workspaceRoot}/knip.config.ts"],
      "options": {
        "command": "NODE_OPTIONS='--import=tsx/esm' knip --workspace {projectRoot}",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "cache": true,
      "configurations": {
        "check": {},
        "write": {
          "args": "--fix"
        }
      },
      "defaultConfiguration": "check",
      "inputs": [
        "default",
        "{workspaceRoot}/eslint.config.base.ts",
        "{projectRoot}/**/*.md"
      ]
    },
    "markdown-lint": {
      "cache": true,
      "configurations": {
        "check": {
          "command": "markdownlint-cli2 '{projectRoot}/**/*.md'"
        },
        "write": {
          "command": "markdownlint-cli2 --fix '{projectRoot}/**/*.md'"
        }
      },
      "defaultConfiguration": "check",
      "executor": "nx:run-commands",
      "inputs": [
        "default",
        "{projectRoot}/**/*.md",
        "{workspaceRoot}/.markdownlint-cli2.jsonc"
      ],
      "options": {
        "command": "markdownlint-cli2 '{projectRoot}/**/*.md'",
        "cwd": "{workspaceRoot}"
      }
    },
    "spell-check": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": [
        "default",
        "{projectRoot}/**/*.{ts,tsx,js,jsx,mts,cts,mjs,cjs,json,jsonc,json5,css,scss,html,yaml,yml,md,mdx}",
        "{workspaceRoot}/cspell.config.yaml"
      ],
      "options": {
        "command": "cspell '{projectRoot}/**/*.{ts,tsx,js,jsx,mts,cts,mjs,cjs,json,jsonc,json5,css,scss,html,yaml,yml,md,mdx}' --no-progress",
        "cwd": "{workspaceRoot}"
      }
    },
    "stylelint": {
      "cache": true,
      "configurations": {
        "check": {
          "command": "stylelint 'src/**/*.css'"
        },
        "write": {
          "command": "stylelint --fix 'src/**/*.css'"
        }
      },
      "defaultConfiguration": "check",
      "executor": "nx:run-commands",
      "inputs": [
        "{projectRoot}/src/**/*.css",
        "{workspaceRoot}/.stylelintrc.cjs"
      ],
      "options": {
        "command": "stylelint 'src/**/*.css'",
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "cache": true,
      "inputs": ["default", "^production"]
    },
    "type-coverage": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": [
        "default",
        "{projectRoot}/**/*.ts",
        "{projectRoot}/**/*.tsx",
        "{projectRoot}/tsconfig.json"
      ],
      "options": {
        "command": "type-coverage --detail > type-coverage-report.txt 2>&1 || (cat type-coverage-report.txt && exit 1)",
        "cwd": "{projectRoot}"
      }
    },
    "typecheck": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": ["typescript"]
        }
      ],
      "options": {
        "command": "tsc --noEmit",
        "cwd": "{projectRoot}"
      }
    },
    "yaml-lint": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": [
        "default",
        "{projectRoot}/**/*.{yaml,yml}",
        "{workspaceRoot}/.yamllint"
      ],
      "options": {
        "command": "yamllint '{projectRoot}'",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tui": {
    "enabled": false
  }
}
