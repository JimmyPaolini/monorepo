{
  "dependencies": {
    "@photostructure/tz-lookup": "^11.4.0",
    "lodash": "^4.17.23",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "^22.5.2",
    "@types/lodash": "^4.17.23",
    "@types/node": "^22.19.1",
    "@vitest/coverage-v8": "^2.1.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "name": "caelundas",
  "scripts": {
    "develop": "set -a && source .env && set +a && tsx src/main.ts",
    "start": "tsx src/main.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:end-to-end": "vitest run --testPathPattern='end-to-end\\.test\\.ts'",
    "test:integration": "vitest run --testPathPattern='integration\\.test\\.ts'",
    "test:unit": "vitest run --testPathPattern='unit\\.test\\.ts'",
    "test:watch": "vitest"
  },
  "type": "module",
  "typeCoverage": {
    "atLeast": 99.46,
    "ignoreCatch": true,
    "ignoreFiles": [
      "**/*.spec.ts",
      "**/*.test.ts",
      "**/testing/**"
    ],
    "strict": true
  },
  "version": "0.0.1"
}
