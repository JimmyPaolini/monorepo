{
  "name": "caelundas",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "develop": "set -a && source .env && set +a && tsx src/main.ts",
    "start": "tsx src/main.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --testPathPattern='unit\\.test\\.ts'",
    "test:integration": "vitest run --testPathPattern='integration\\.test\\.ts'",
    "test:end-to-end": "vitest run --testPathPattern='end-to-end\\.test\\.ts'"
  },
  "dependencies": {
    "@photostructure/tz-lookup": "^11.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "^22.1.2",
    "@types/lodash": "^4.17.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
