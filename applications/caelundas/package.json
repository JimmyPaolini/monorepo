{
  "name": "caelundas",
  "version": "0.0.1",
  "dependencies": {
    "@photostructure/tz-lookup": "^11.3.0",
    "lodash": "^4.17.23",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "^22.1.3",
    "@types/lodash": "^4.17.23",
    "@types/node": "^22.19.1",
    "@vitest/coverage-v8": "^2.1.8",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "scripts": {
    "develop": "set -a && source .env && set +a && tsx src/main.ts",
    "start": "tsx src/main.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:end-to-end": "vitest run --testPathPattern='end-to-end\\.test\\.ts'",
    "test:integration": "vitest run --testPathPattern='integration\\.test\\.ts'",
    "test:unit": "vitest run --testPathPattern='unit\\.test\\.ts'",
    "test:watch": "vitest"
  },
  "type": "module",
  "typeCoverage": {
    "atLeast": 99.46,
    "strict": true,
    "ignoreCatch": true,
    "ignoreFiles": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/testing/**"
    ]
  }
}
