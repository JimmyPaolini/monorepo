{
  "MD013": {
    "line_length": 120,
    "code_blocks": false,
    "headers": false,
    "code_fence_style": false,
    "tables": false,
    "strict": false
  },
  "MD033": {
    "allowed_elements": ["img", "a"]
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "heading-style": {
    "style": "atx"
  },
  "ul-style": {
    "style": "dash"
  },
  "list-marker-space": {
    "ul_single": 1,
    "ol_single": 1
  },
  "MD040": false
}
